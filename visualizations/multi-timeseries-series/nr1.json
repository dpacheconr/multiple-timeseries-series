{
    "schemaType": "VISUALIZATION",
    "id": "multi-timeseries-series",
    "displayName": "test: period comparison",
    "description": "Displays timeseries data from different periods on the same chart",
    "configuration": [
        {
            "name": "conf_accountId",
            "title": "Account ID",
            "description": "Account ID to be associated with the query",
            "type": "account-id"
        },
        {
            "name": "conf_query",
            "title": "Query",
            "description": "NRQL query for visualization",
            "type": "nrql"
        },
        {
            "name": "conf_compare",
            "title": "Compare",
            "description": "Number minutes/hours/days/weeks to compare",
            "type": "number"
        },
        {
            "name": "conf_timeseries",
            "title": "Timeseries bucket size",
            "description": "minutes/days/hours/weeks/years",
            "type": "string"
        },
        {
            "name": "conf_hideoriginaldata",
            "title": "Hide NRQL Data",
            "description": "Hide NRQL data",
            "type": "boolean"
        },
        {
            "name": "conf_average",
            "title": "Average",
            "description": "Average line",
            "type": "boolean"
        },
        {
            "name": "conf_minmaxareabol",
            "title": "Min Max Area",
            "description": "Min Max Area",
            "type": "boolean"
        },
        {
            "name": "conf_trimmedareabol",
            "title": "Trimmed Area",
            "description": "Trimmed Area",
            "type": "boolean"
        },
        {
            "name": "conf_trimpercent",
            "title": "Trim Percentage",
            "description": "Percentage to trim from min and max values, defaults to 10%",
            "type": "string"
        },
        {
            "name": "conf_clippedareabol",
            "title": "Clipped Area",
            "description": "Clipped Area",
            "type": "boolean"
        },
        {
            "name": "conf_clipsize",
            "title": "Clip size",
            "description": "Number of pointsto clip from data set",
            "type": "string"
        },
        {
            "name": "conf_alignment",
            "title": "Alignment",
            "description": "How the timeseries should be aligned",
            "type": "enum",
            "items": [
                {
                    "title": "Start: Align by timeseries start points",
                    "value": "start"
                },
                {
                    "title": "Middle: Align by timeseries mid points ",
                    "value": "middle"
                },
                {
                    "title": "End: Align by timeseries end points",
                    "value": "end"
                }
            ]
        }
    ]
}